<h1>New Post</h1>

<%= form_with :model => @post do |form| %>
  <p>
    <%= form.label "title" %>
    <%= form.text_field "title" %>
  </p>
  <p>
    <%= form.label "entry" %>
    <%= form.text_field "entry" %>
  </p>
  <p>
    <%= form.label "date" %>
    <%= form.date_field "date" %>
  </p>
  <p>
    <%= form.submit %>
  </p>
<% end %>